{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "plugin:prettier/recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"]
  }
}
